{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/build.js production",
    "build:dev": "node scripts/build.js development",
    "build:staging": "node scripts/build.js staging",
    "build:production": "node scripts/build.js production",
    "build:analyze": "npm run build && node scripts/analyze-bundle.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "preview": "vite preview",
    "preview:staging": "vite preview --mode staging",
    "preview:production": "vite preview --mode production",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:integration": "vitest run src/test/integration",
    "test:unit": "vitest run --exclude src/test/integration",
    "test:e2e": "npm run test:integration",
    "deploy:staging": "npm run build:staging && npm run deploy:upload:staging",
    "deploy:production": "npm run build:production && npm run optimize:production && npm run deploy:upload:production",
    "deploy:upload:staging": "echo 'Upload to staging server'",
    "deploy:upload:production": "echo 'Upload to production server'",
    "optimize:production": "node scripts/optimize-production.js",
    "check:deployment": "node scripts/deployment-readiness.js",
    "prepare:deploy": "npm run build:production && npm run optimize:production && npm run check:deployment",
    "health-check": "node scripts/health-check.js",
    "validate-build": "node scripts/validate-build.js"
  },
  "dependencies": {
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.536.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.61.1",
    "react-router-dom": "^7.7.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chart.js": "^2.9.41",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "gzip-size": "^7.0.0",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.0",
    "msw": "^2.10.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "puppeteer": "^24.15.0",
    "tailwindcss": "^3.4.17",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
