# Production Environment Configuration
NODE_ENV=production
VITE_NODE_ENV=production

# Application Information
VITE_APP_NAME=EduAI
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI-powered personalized education platform

# API Configuration
VITE_API_BASE_URL=https://api.eduai.com
VITE_API_TIMEOUT=10000
VITE_RETRY_ATTEMPTS=3

# Feature Flags
VITE_FEATURE_OFFLINE_SUPPORT=true
VITE_FEATURE_SERVICE_WORKER=true
VITE_FEATURE_PUSH_NOTIFICATIONS=true
VITE_FEATURE_DARK_MODE=true
VITE_FEATURE_BETA_FEATURES=false

# Performance Configuration
VITE_CACHE_DURATION=300000
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Security
VITE_SECURE_COOKIES=true
VITE_CSRF_PROTECTION=true

# CDN and Assets
VITE_CDN_URL=https://cdn.eduai.com
VITE_STATIC_ASSETS_URL=https://assets.eduai.com

# Social/SEO
VITE_SITE_URL=https://eduai.com
VITE_SOCIAL_IMAGE=https://eduai.com/og-image.png
VITE_TWITTER_HANDLE=@eduai

# Monitoring
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=
VITE_HOTJAR_ID=
