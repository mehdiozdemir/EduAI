VITE_STATIC_ASSETS_URL=

# Social/SEO
VITE_SITE_URL=http://localhost:5173
VITE_SOCIAL_IMAGE=
VITE_TWITTER_HANDLE=

# Monitoring
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=
