# Development Environment Configuration
NODE_ENV=development
VITE_NODE_ENV=development

# Application Information
VITE_APP_NAME=EduAI (Dev)
VITE_APP_VERSION=1.0.0-dev
VITE_APP_DESCRIPTION=AI-powered personalized education platform - Development

# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_API_TIMEOUT=30000
VITE_RETRY_ATTEMPTS=1

# Feature Flags
VITE_FEATURE_OFFLINE_SUPPORT=false
VITE_FEATURE_SERVICE_WORKER=false
VITE_FEATURE_PUSH_NOTIFICATIONS=false
